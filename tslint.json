{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [
      true,
      "single"
    ],
    "indent": [
      true
    ],
    "interface-name": [
      false
    ],
    "arrow-parens": false,
    "object-literal-sort-keys": false,
    "no-unused-expression": [true, "allow-fast-null-checks"],
    "no-var-requires": false,
    "no-bitwise": false,
    "no-console": false
  },
  "rulesDirectory": []
}